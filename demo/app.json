{
  "expo": {
    "name": "CarSight Demo",
    "slug": "carsight-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#0a0a0f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carsight.demo",
      "infoPlist": {
        "NFCReaderUsageDescription": "This app uses NFC to sync with your car's CarTag device"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0a0a0f"
      },
      "package": "com.carsight.demo",
      "permissions": [
        "android.permission.NFC"
      ]
    },
    "plugins": [
      "expo-dev-client",
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "This app uses NFC to sync with your car's CarTag device"
        }
      ]
    ]
  }
}
